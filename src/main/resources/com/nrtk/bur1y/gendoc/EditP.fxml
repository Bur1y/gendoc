<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity"
      xmlns="http://javafx.com/javafx/17" fx:controller="com.nrtk.bur1y.gendoc.Controllers.EditP"
      xmlns:fx="http://javafx.com/fxml/1">
    <children>
        <HBox VBox.vgrow="ALWAYS">
            <children>
                <VBox>
                    <children>
                        <Label text="ФИО"/>
                        <TextField fx:id="fio"/>
                    </children>
                    <HBox.margin>
                        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
                    </HBox.margin>
                </VBox>
                <VBox>
                    <children>
                        <Label text="МДК 01"/>
                        <TextField fx:id="mdk1"/>
                    </children>
                    <HBox.margin>
                        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
                    </HBox.margin>
                </VBox>
                <VBox>
                    <children>
                        <Label text="МДК 02"/>
                        <TextField fx:id="mdk2"/>
                    </children>
                    <HBox.margin>
                        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
                    </HBox.margin>
                </VBox>
                <VBox>
                    <children>
                        <Label text="МДК 03"/>
                        <TextField fx:id="mdk3"/>
                    </children>
                    <HBox.margin>
                        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
                    </HBox.margin>
                </VBox>
                <VBox>
                    <children>
                        <Label text="КП"/>
                        <TextField fx:id="kp"/>
                    </children>
                    <HBox.margin>
                        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
                    </HBox.margin>
                </VBox>
                <VBox>
                    <children>
                        <Label text="УП"/>
                        <TextField fx:id="up"/>
                    </children>
                    <HBox.margin>
                        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
                    </HBox.margin>
                </VBox>
                <VBox>
                    <children>
                        <Label text="ПП"/>
                        <TextField fx:id="pp"/>
                    </children>
                    <HBox.margin>
                        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
                    </HBox.margin>
                </VBox>
            </children>
        </HBox>
        <HBox>
            <children>
                <Button maxWidth="1.7976931348623157E308" mnemonicParsing="false" onAction="#back" text="Отмена" HBox.hgrow="ALWAYS">
                    <HBox.margin>
                        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0"/>
                    </HBox.margin>
                </Button>
                <Button maxWidth="1.7976931348623157E308" mnemonicParsing="false" onAction="#save" text="Сохранить" HBox.hgrow="ALWAYS">
                    <HBox.margin>
                        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0"/>
                    </HBox.margin>
                </Button>
            </children>
        </HBox>
    </children>
    <padding>
        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
    </padding>
</VBox>
